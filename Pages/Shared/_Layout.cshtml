<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lingzu</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/carousel.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Lingzu.styles.css" asp-append-version="true" />
    <!--Font Awesome-->
    <link rel="stylesheet" href="~/font-awesome/css/all.css" asp-append-version="true" />
</head>
<body>
    <header>
      <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-page="/Index">Lingzu</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav me-auto mb-2 mb-md-0">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" asp-area="" asp-page="/Index">Inicio</a>
              </li>
              <li class="nav-item">
                  <a class="nav-link" asp-area="" asp-page="/Productos/Index">Productos</a>
              </li>
              <li class="nav-item">
                 <a class="nav-link" asp-area="" asp-page="/Clientes/Index" tabindex="-1">Clientes</a>
              </li>
              <li class="nav-item">
                 <a class="nav-link" asp-area="" asp-page="/Ventas/Index" tabindex="-1">Ventas</a>
              </li>
              
               <li class="nav-item">
                 <a class="nav-link" asp-area="" asp-page="/Contacto" tabindex="-1">Contactos</a>
              </li>

            </ul>
          </div>
        </div>
      </nav>
    </header>

    <main>
            @RenderBody()
    </main>

    <footer class="border-top footer text-muted bg-dark">
        <ul class="nav justify-content-center border-bottom pb-3 mb-3">
            <li class="nav-item">
                <a class="nav-link" asp-area="" asp-page="/Privacy" tabindex="-1">Política de Privacidad</a>
            </li>
           


        </ul>
        <p class="text-center text-body-secondary">&copy; 2023 Lingzu, Inc</p>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>